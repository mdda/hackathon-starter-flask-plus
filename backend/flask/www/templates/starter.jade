    title Starter Template for Bootstrap
    
    // Bootstrap core CSS 
    link(href='/static/css/bootstrap.min.css', rel='stylesheet')
    // Custom styles for this template 
    link(href='/static/css/starter-template.css', rel='stylesheet')
